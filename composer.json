{
  "name": "harry/crud-package",
  "description": "A package to handle Api CRUD operations in Laravel",
  "minimum-stability": "stable",
  "license": "proprietary",
  "type": "library",
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "harry\\CrudPackage\\": "packages/harry/CrudPackage/src/"
    }
  },
  "authors": [
    {
      "name": "harry",
      "email": "pantharish120@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "laravel/framework": "^8.0|^9.0|^10.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "harry\\CrudPackage\\CrudPackageServiceProvider"
      ]
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "packages/harry/CrudPackage"
    }
  ]
}
