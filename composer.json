{
  "name": "harryes/crudpackage",
  "description": "A package to handle Api CRUD operations in Laravel",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "library",
  "keywords": ["laravel", "crud", "api", "generator", "package", "laravel-package", "dynamic", "crud-generator"],
  "autoload": {
    "psr-4": {
      "Harryes\\CrudPackage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Harryes\\CrudPackage\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Harry Es Pant",
      "email": "pantharish120@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "fakerphp/faker": "^1.9",
    "orchestra/testbench": "^6.0",
    "vlucas/phpdotenv": "^5.3"
  },
  "scripts": {
    "post-autoload-dump": []
  },
  "extra": {
    "laravel": {
      "providers": [
        "Harryes\\CrudPackage\\CrudPackageServiceProvider"
      ]
    }
  },
  "prefer-stable": true
}
